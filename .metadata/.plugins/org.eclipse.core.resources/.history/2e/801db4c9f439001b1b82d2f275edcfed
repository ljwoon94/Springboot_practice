$("#pubBtn").on("click",funtion(){
	var boardNo = ${"#boardNo"}; 
	var title = ${"#title"};
	var content = ${"#content"}; 
	var writer = ${"#writer"};
	
	var boardNoVal = boardNo.val();
	var contentVal = content.val();
	var writerVal = writer.val();
	
	var boardObject = {
		boardNo : boardNoVal,
		title : titleVal,
		content : contentVal,
		writer : writerVal
	}
}

)